import * as Blockly from 'blockly/core';
import 'blockly/python';
import {CategoryColors} from '@/global/colors';

Blockly.Blocks['dqn_model_inference_1'] = {
    init: function () {
        this.jsonInit({
            "inputsInline": true,
            "nextStatement": null,
            "previousStatement": null,
            "colour": CategoryColors.AI,
            "message0": '1、DQN模型推理图形块 %1 %2',
            "args0": [{
                "check": "Number",
                "type": "input_value",
                "name": "duration"
            }, {
                "name": "ms_us",
                "options": [
                    [Blockly.Msg.mpython_s, 's'],
                    [Blockly.Msg.MPYTHON_MS, 'ms'],
                    [Blockly.Msg.MPYTHON_US, 'us']
                ],
                "type": "field_dropdown"
            }]
        });
    }
};
Blockly.Python['dqn_model_inference_1'] = function (block) {
    Blockly.Python.definitions_['import_time'] = 'import time';
    var value_duration = Blockly.Python.valueToCode(block, 'duration', Blockly.Python.ORDER_ATOMIC);
    var ms_us = block.getFieldValue('ms_us');
    switch (ms_us) {
        case 'ms':
        case 'us':
            return 'time.sleep_' + ms_us + '(' + value_duration + ')\n';
        case 's':
            return 'time.sleep(' + value_duration + ')\n';
        default:
            return '';
    }
};

